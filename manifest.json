{
    "manifest_version": 3,
    "name": "War Room Dark Mode",
    "version": "1.1.0",
    "description": "Add dark mode to War Room!",
    "content_scripts": [
        {
            "js": ["darkmode.js"],
            "css": ["darkmode.css"],
            "matches": [
                "https://war.elk.sh/*"
            ]
        }
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}